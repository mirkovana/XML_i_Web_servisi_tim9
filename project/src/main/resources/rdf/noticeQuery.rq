SELECT * FROM <http://localhost:8081/fuseki/OrganVlasti/data/noticeMetadata> WHERE {
	?obavestenje <http://www.ftn.uns.ac.rs/predicate/ime> ?ime .
	?obavestenje <http://www.ftn.uns.ac.rs/predicate/prezime> ?prezime .
	?obavestenje <http://www.ftn.uns.ac.rs/predicate/datum> ?datum .
	?obavestenje <<http://www.ftn.uns.ac.rs/predicate/brojPredmeta> ?broj_predmeta .
	?obavestenje <<http://www.ftn.uns.ac.rs/predicate/brojPredmeta> ?sedisteOrgana .
	?obavestenje <<http://www.ftn.uns.ac.rs/predicate/brojPredmeta> ?nazivOrgana .
	FILTER( CONTAINS(UCASE(str(?ime)), UCASE("{{ime}}"))
    && CONTAINS(UCASE(str(?prezime)), UCASE("{{prezime}}"))
    && CONTAINS(UCASE(str(?datum)), UCASE("{{datum}}"))
    && CONTAINS(UCASE(str(?broj_predmeta)), UCASE("{{broj_predmeta}}"))
    && CONTAINS(UCASE(str(?broj_predmeta)), UCASE("{{sedisteOrgana}}"))
    && CONTAINS(UCASE(str(?broj_predmeta)), UCASE("{{nazivOrgana}}"))
  ) .
}